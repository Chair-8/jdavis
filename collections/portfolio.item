<div class="portfolio_item">
  <div class="portfolio_banner" style="background-image: url({item.assetUrl}?format=2500w); background-size: cover; background-position: center; background-repeat: no-repeat;">

  </div>
  <div class="portfolio_stats">
    <div class="stats_left">
      <h1>{item.title}</h1>
      {item.excerpt}
    </div>
    <div class="stats_right">
      <h2>Stats</h2>
      <div class="stats_row">
        <h3>Client</h3><p>{item.customContent.area}</p>
      </div>
      <div class="stats_row">
        <h3>Use</h3><p>{item.customContent.awards}</p>
      </div>
      <div class="stats_row">
        <h3>Details</h3><p>{item.customContent.locations}</p>
      </div>
      <div class="stats_row">
        <h3>Services</h3><p>{item.customContent.disciplines}</p>
      </div>
      <div class="stats_row">
        <h3>Completion</h3><p>{item.customContent.completion}</p>
      </div>
    </div>
  </div>
  <div class="portfolio_content">
    <div class="grid-container">
      <div class="portfolio_image image_one"></div>
      <div class="portfolio_image image_two"></div>
      <div class="portfolio_image image_three"></div>
      <div class="text_problem"><strong>The Problem</strong><br>{item.customContent.problem}</div>
      <div class="portfolio_image image_four"></div>
      <div class="text_solution"><strong>The Solution</strong><br>{item.customContent.solution}</div>
      <div class="portfolio_image image_five"></div>
      <div class="portfolio_image image_six"></div>
      <div class="text_result"><strong>The Result</strong><br>{item.customContent.result}</div>
    </div>
  </div>
  <div class="project_content">
    <div class="grid-container">
    {.repeated section item.customContent.projectGallery}
    <div class=" content-fill"><img class="content-fill" {@|image-meta} /></div>
    {@}
    {item|assetUrl}
    {.end}
    </div>
  </div>
</div>

<script>
function init(){
var url = window.location.href
$.getJSON( url + '?format=json-pretty', function( data ) {
console.log(data);
data.item.customContent.gallery.forEach(function(a, i){
$('.portfolio_image').eq(i).attr('style', 'background-image: url('+a.assetUrl+'?format=2500w)')
})
})
}
init()
</script>
