<div class="portfolio_items">
  {.repeated section items}
  <div class="portfolio_item {categories}">
    {.excerpt?}
      {.main-image?}
      <div class="excerpt-thumb">
        <a href="{fullUrl}" class="excerpt-image content-fill"><img {@|image-meta} /></a>
      </div>
      {.end}
      <div class="item_overlay">
      <h1 class="title">{title}</h1>
      <div class="summary">{excerpt}</div>
      <h3 class="entry-more-link"><a data-read-more="View Project" href="{fullUrl}">View Project</a></h3>
      </div>
    {.or}
    {.end}
  </div>
  {.end}
</div>

<script>
$('.portfolio_item').each(function(){
var t = $(this).attr('class').split(' ')[1];
var $th = $(this)
  $(this).attr('class').split(' ').forEach(function(a, i){
  $th.removeClass(a)
a.split(',').forEach(function(b, i){
    if(b == "portfolio_item"){
    $th.addClass('portfolio_item')
    }else{
    $th.addClass(b.replace(/[^a-zA-Z ]/g, "").replace(/\s/g, '').toLowerCase())
    }
  })
})
})
</script>
